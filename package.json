{
  "name": "gwvp",
  "version": "0.1.3",
  "description": "webpage builder",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testw": "jest --watch",
    "testc": "jest --coverage",
    "build": "webpack --display-error-details --config webpack.prod.js",
    "start": "webpack-dev-server --open --hot --config webpack.dev.js",
    "docs": "documentation build src/js/** -f html -o dist/docs",
    "license": "license-checker --production",
    "lints": "sass-lint -c ./sass-lint.yml ./src/style/**/*.scss -v -q",
    "lintj": "eslint -c ./.eslintrc.js ./src/js"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "author": "Wladislaw Koschemako",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "documentation": "^5.3.5",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jquery": "^3.2.1",
    "license-checker": "^15.0.0",
    "node-sass": "^4.6.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.2"
  },
